{
  "name": "mna-template-server",
  "version": "0.0.1",
  "description": "[MNA] App Template",
  "repository": "https://github.com/mission-apprentissage/template-apprentissage.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "mocha --recursive --exit --timeout 5000 tests/",
    "lint": "eslint src/",
    "docker:start": "docker-compose up -d --build --force-recreate",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes"
  },
  "dependencies": {
    "bunyan": "1.8.12",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "config": "3.3.1",
    "env-var": "6.1.1",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "sha512crypt-node": "0.1.0",
    "xlsx": "0.16.1",
    "json2csv": "5.0.1",
    "convert-csv-to-json": "0.0.15",
    "jsonwebtoken": "8.5.1"
  },
  "engines": {
    "node": ">=12.11.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "axiosist": "0.9.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "mocha": "7.1.2",
    "nodemon": "2.0.3",
    "prettier": "2.0.5"
  }
}
