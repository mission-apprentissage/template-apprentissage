{
  "name": "mna-mnaprojectname-server",
  "version": "0.0.1",
  "description": "[MNA] App Template",
  "repository": "https://github.com/mission-apprentissage/mnaprojectname.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "MNAPROJECTNAMEL_LOG_LEVEL=fatal mocha tests/**/*-test.js",
    "cli": "MNAPROJECTNAMEL_LOG_DESTINATIONS=stderr node src/cli.js",
    "coverage": "nyc --temp-dir .coverage/.nyc_output --report-dir .coverage --reporter=lcov --reporter=html yarn test",
    "lint": "eslint src/ tests/"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "commander": "9.2.0",
    "compose-middleware": "5.0.1",
    "config": "3.3.1",
    "convert-csv-to-json": "0.0.15",
    "env-var": "6.1.1",
    "express": "4.17.1",
    "fs-extra": "9.0.0",
    "joi": "17.2.1",
    "json2csv": "5.0.1",
    "lodash": "4.17.15",
    "luxon": "2.3.2",
    "mongoose": "5.10.2",
    "mongoose-paginate": "5.0.3"
  },
  "engines": {
    "node": ">=12.11.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "axiosist": "0.9.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "lint-staged": ">=10",
    "mocha": "7.1.2",
    "nodemon": "2.0.3",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "pretty-ms": "7.0.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
